    "@commitlint/config-conventional": "^20.2.0",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "commitizen": "^4.3.1",
    "cspell": "^9.4.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
